<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="640.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.vad1mchk.progr.lab05.client.controllers.LoginController">
   <children>
      <VBox alignment="TOP_CENTER" spacing="32.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Text fx:id="loginHeader" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <Text fx:id="login_username" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter username:">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Text>
                  <TextField>
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
                  <PasswordField>
                     <font>
                        <Font size="16.0" />
                     </font>
                  </PasswordField>
               </children>
            </VBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="32.0" left="50.0" right="50.0" top="32.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
